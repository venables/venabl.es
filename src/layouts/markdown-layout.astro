---
import Article from "@/components/articles/article.astro"
import Layout from "./layout.astro"

interface Props {
  content: {
    title: string
    description: string
    date: string
  }
}

const { content } = Astro.props
---

<Layout title={content.title} description={content.description}>
  <Article>
    <slot />
  </Article>
</Layout>
