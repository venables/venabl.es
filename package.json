    "parcel-bundler": "^1.12.3",
    "parcel-plugin-inliner": "^1.0.10",
    "postcss": "^7.0.31",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.4.6",
    "tailwindcss-font-inter": "^1.0.8"
  }
}
