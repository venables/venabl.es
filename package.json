{
  "name": "venabl.es",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn clean && yarn build:parcel && cp src/CNAME ./docs/",
    "build:parcel": "parcel build src/index.html -d docs --no-source-maps",
