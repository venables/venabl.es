{
  "name": "@venables/website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "pnpm run format:check && pnpm run lint && pnpm run typecheck && pnpm run test",
    "clean": "git clean -xdf .astro node_modules/.cache",
    "dev": "astro dev",
    "fix": "pnpm run format && pnpm run lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "nuke": "pnpm run clean; git clean -xdf node_modules",
    "outdated": "pnpm dlx npm-check-updates --interactive --format group",
    "preview": "astro preview",
    "test": "vitest",
    "typecheck": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.2",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.2",
    "@astrojs/vercel": "^8.2.4",
    "@fontsource-variable/geist-mono": "^5.2.6",
    "@fontsource-variable/jetbrains-mono": "^5.2.6",
    "@fontsource/ia-writer-quattro": "^5.2.5",
    "@lucide/astro": "^0.535.0",
    "astro": "^5.12.7",
    "astro-seo": "^0.8.4",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "valibot": "^1.1.0",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@eslint/js": "^9",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/eslint-config-prettier": "^6",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1",
    "eslint-plugin-import-x": "^4",
    "eslint-plugin-jsx-a11y": "^6",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  }
}
